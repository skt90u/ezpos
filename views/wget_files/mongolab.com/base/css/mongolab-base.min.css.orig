html{background-color:#515c65;color:white;}
body{margin:0;-webkit-text-size-adjust:auto;}
body{font-family:"Droid Sans",sans-serif !important;font-size:14px;}
a:link,a:visited{color:inherit;}
p{margin-top:1em;margin-bottom:1em;line-height:1.5em;}
p:first-child{margin-top:0;}
li{list-style-type:inherit;margin-left:30px;padding-bottom:5px;}
img{border:none;}
.clearer{clear:both;}
*{text-align:left;}
strong,em{font-weight:bold;}
h1{font-family:"Museo Sans",sans-serif !important;font-size:28px;font-weight:normal;color:white;margin-top:30px;margin-bottom:20px;}
h1:first-child{margin-top:0;}
h2{font-family:"Museo Sans",sans-serif !important;font-size:18px;font-weight:normal;color:#90e8fe;margin-top:30px;margin-bottom:20px;}
h2:first-child{margin-top:0;}
h3{font-family:"Museo Sans",sans-serif !important;font-size:18px;font-weight:bold;letter-spacing:0.75px;}
.overlay{display:none;z-index:10000;-moz-box-shadow:0 0 90px 5px #000;-webkit-box-shadow:0 0 90px #000;border:1px solid #79ccf5;}
#header{font-family:"Museo Sans",sans-serif !important;font-size:15px;color:white;text-align:right;border-bottom:1px solid #5a6066;background:#2e3843;background:-moz-linear-gradient(top, #2e3843 0%, #424d56 100%);background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #2e3843), color-stop(100%, #424d56));background:-webkit-linear-gradient(top, #2e3843 0%, #424d56 100%);background:-o-linear-gradient(top, #2e3843 0%, #424d56 100%);background:-ms-linear-gradient(top, #2e3843 0%, #424d56 100%);background:linear-gradient(to bottom, #2e3843 0%, #424d56 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#2e3843', endColorstr='#424d56', GradientType=0);}
#header .wrap{min-width:960px;margin:0 auto;padding:0 30px;}
#header .logo{float:left;}
#header .logo a{background-repeat:no-repeat;text-indent:-9999px;display:block;}
#header .links{font-size:17px;text-align:right;}
#header .links a{text-decoration:none;margin-left:20px;line-height:26px;font-weight:normal;}
#header .links a.signup{font-family:"Museo Sans",sans-serif !important;font-size:15px;font-weight:bold;color:#ffffff;padding:6px 17px;text-decoration:none;background:-moz-linear-gradient(top, #e3aa00 0%, #d68f00);background:-webkit-gradient(linear, left top, left bottom, from(#e3aa00), to(#d68f00));-webkit-border-radius:5px;-moz-border-radius:5px;-ms-border-radius:5px;-o-border-radius:5px;border-radius:5px;-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);cursor:pointer;margin-left:20px;}#header .links a.signup:hover{background:-moz-linear-gradient(top, #edb204 0%, #bc7300);background:-webkit-gradient(linear, left top, left bottom, from(#edb204), to(#bc7300));-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);}
#header .links a.signup:active{background:-moz-linear-gradient(top, #cc8600 0%, #cc8600);background:-webkit-gradient(linear, left top, left bottom, from(#cc8600), to(#cc8600));-moz-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);-webkit-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);}
#header .links a.login{font-family:"Museo Sans",sans-serif !important;text-decoration:none;font-size:15px;font-weight:bold;color:#ffffff;padding:6px 17px;background:-moz-linear-gradient(top, #58afd9 0%, #3a97cb);background:-webkit-gradient(linear, left top, left bottom, from(#58afd9), to(#3a97cb));-webkit-border-radius:5px;-moz-border-radius:5px;-ms-border-radius:5px;-o-border-radius:5px;border-radius:5px;-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);cursor:pointer;margin-left:20px;}#header .links a.login:hover{background:-moz-linear-gradient(top, #78c3e4 0%, #227ab9);background:-webkit-gradient(linear, left top, left bottom, from(#78c3e4), to(#227ab9));-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);}
#header .links a.login:active{background:-moz-linear-gradient(top, #43a1d2 0%, #43a1d2);background:-webkit-gradient(linear, left top, left bottom, from(#43a1d2), to(#43a1d2));-moz-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);-webkit-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);}
#header{height:117px;}
#header .logo{padding-top:25px;}
#header .logo a{background-image:url("/base/img/mongolab-logo-250x65.png");width:250px;height:65px;}
#header .links{padding-top:48px;}
#nav{font-family:"Museo Sans",sans-serif !important;font-size:15px;color:white;text-align:right;border-top:1px solid #000;background:#9199a2;background:-moz-linear-gradient(top, #9199a2 0%, #596069 100%);background:-webkit-gradient(linear, left top, left bottom, color-stop(0%, #9199a2), color-stop(100%, #596069));background:-webkit-linear-gradient(top, #9199a2 0%, #596069 100%);background:-o-linear-gradient(top, #9199a2 0%, #596069 100%);background:-ms-linear-gradient(top, #9199a2 0%, #596069 100%);background:linear-gradient(to bottom, #9199a2 0%, #596069 100%);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#9199a2', endColorstr='#596069', GradientType=0);}
#nav{height:70px;font-size:25px;}
#nav .wrap{width:960px;margin:0 auto;}
#nav .wrap div{padding-top:23px;text-transform:none;font-weight:bold;}
#content{background-color:#1b242c;color:#8c969d;}
#content .wrap{width:960px;margin:0 auto;padding-top:40px;padding-bottom:40px;}
#content .wrap #mainCol{width:720px;padding-right:20px;float:left;}
#content .wrap #sidebar{width:220px;float:left;}
#preFooterBar{font-family:"Museo Sans",sans-serif !important;font-size:15px;color:white;text-align:right;height:84px;border-bottom:1px solid #2e353c;border-top:1px solid #636a73;background:#3f4852;}
#preFooterBar .wrap{margin-left:100px;padding-left:0px;}
#preFooterBar .logo{float:left;padding:22px 0 0 0;}
#preFooterBar .logo img{width:146px;height:38px;}
#preFooterBar .links{color:#9eaab2;font-size:15px;padding-top:30px;text-align:right;}
#preFooterBar .links strong{color:white;}
#preFooterBar .links a{text-decoration:none;margin-left:20px;line-height:26px;font-weight:bold;}
#preFooterBar .links a.signup{font-family:"Museo Sans",sans-serif !important;font-size:15px;font-weight:bold;color:#ffffff;padding:6px 17px;text-decoration:none;background:-moz-linear-gradient(top, #e3aa00 0%, #d68f00);background:-webkit-gradient(linear, left top, left bottom, from(#e3aa00), to(#d68f00));-webkit-border-radius:5px;-moz-border-radius:5px;-ms-border-radius:5px;-o-border-radius:5px;border-radius:5px;-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.5),inset 0px 1px 0px rgba(255, 255, 255, 0.4);cursor:pointer;}#preFooterBar .links a.signup:hover{background:-moz-linear-gradient(top, #edb204 0%, #bc7300);background:-webkit-gradient(linear, left top, left bottom, from(#edb204), to(#bc7300));-moz-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);-webkit-box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);box-shadow:0px 1px 1px rgba(0, 0, 0, 0.6),inset 0px 1px 3px rgba(255, 255, 255, 0.6);}
#preFooterBar .links a.signup:active{background:-moz-linear-gradient(top, #cc8600 0%, #cc8600);background:-webkit-gradient(linear, left top, left bottom, from(#cc8600), to(#cc8600));-moz-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);-webkit-box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);box-shadow:0px 1px 0px rgba(0, 0, 0, 0.3),inset 0px 1px 2px rgba(0, 0, 0, 0.1);}
#footer{background-color:#515c65;border-top:1px solid #636a73;color:white;font:13px "Museo Sans",sans-serif;padding-bottom:30px;}
#footer .wrap{margin-left:100px;padding-left:0px;}
#footer .siteLinks{color:white;margin-bottom:25px;}
#footer .siteLinks strong{color:#fff;font-size:14px;}
#footer .siteLinks a{color:#fff;text-decoration:none;}
#footer .siteLinks ul{margin:0;padding:0;list-style:none;}
#footer .siteLinks li{float:left;margin:7px 30px 0 0;padding:0;width:139px;}
#footer .siteLinks li.certs{margin-top:31px;}
#footer .powered{margin-top:35px;}
#footer .copyright{margin-top:0px;font-size:11px;color:#91989e;}
#footer .copyright a{margin-left:10px;color:#91989e;}
#SnapABug_WP table{color:#666;}
#SnapABug_WP tr:hover{background-color:transparent;}
#SnapABug_WP td{border:0px;}
#SnapABug_PB,#SnapABug_P img{display:none;}
